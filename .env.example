DOCLING_ENV=TST  # or PPD/PRD
DOCLING_URL="https://your-docling-host.example.com/cbai/v1/docling"  # optional override
DOCLING_API_KEY_VAR=CBAI_API_KEY_TST  # optional override
DOCLING_PDF_PATH="data/sample.pdf"
DOCLING_EXPORT_TYPE=markdown
DOCLING_CHUNKING_TYPE=hybrid
DOCLING_PICTURE_MODEL=""
DOCLING_PICTURE_PROMPT="Describe each figure."
DOCLING_MAX_TOKEN_PER_CHUNK=7500
DOCLING_POLL_INTERVAL=10
DOCLING_POLL_ATTEMPTS=120

LLMSHERPA_ENV=TST  # or PPD/PRD
LLMSHERPA_URL="https://api-tst.example.com/cbai/v1/llm_sherpa"  # optional override
LLMSHERPA_API_KEY_VAR=CBAI_API_KEY_TST  # optional override
LLMSHERPA_ENDPOINT="parsing/"
LLMSHERPA_QUERY="renderFormat=all&applyOcr=no"
LLMSHERPA_PDF_PATH="data/sample.pdf"
LLMSHERPA_PRESERVE_LAYOUT=true
LLMSHERPA_CHUNK_SIZE=800
LLMSHERPA_CHUNK_OVERLAP=100
LLMSHERPA_TIMEOUT=120

RUN_LABEL="baseline-run"
RUN_NOTES="Docling chunk=1500 vs Sherpa default"

# Azure OpenAI (GPT-5 parsing)
AZURE_OPENAI_ENDPOINT=""
AZURE_OPENAI_API_KEY=""
AZURE_OPENAI_GPT5_DEPLOYMENT=""
